  <script>

    var PlanetList = ["Terra", "Marte", "Plutão", "Vênus", "Júpiter", "Saturno"]

    var planet = prompt("Digite o planet que voce deseja colocar na lista: ")

    if (PlanetList.includes(planet)) {
      alert("O " + planet + " esta na lista")
    } else {
      alert("O " + planet + " nao esta na lista")
    }

    console.log(PlanetList)
    alert(PlanetList)

  </script>
